dqzf:dialog
{
	label="带圈字符";
	:row
	{
		:image
		{
			key="i";
			color=-2;
			width=25;
			height=5;
			fixed_height=true;
		}
		:column
		{
			:edit_box
			{
				label="输入文字：";
				key="wz";
				value="文字";
				edit_width=25;
			}
			:popup_list
			{
				label="选择比例：";
				key="b";
				list="1:10\n1:20\n1:30\n1:35\n1:50\n1:80\n1:100\n1:150\n1:200";
				value="2";
			}
			:boxed_column
			{
				label="引线";
				:text
				{
					label="是否添加引线？";
				}
				:row
				{
					:radio_button
					{
						label="是";
						key="sy";
					}
					:radio_button
					{
						label="否";
						key="fy";
					}
				}
				:boxed_column
				{
					:text
					{
						label="引线位置";
					}
					:row
					{
						:radio_button
						{
							label="上";
							key="s";
						}
						:radio_button
						{
							label="下";
							key="x";
						}
						:radio_button
						{
							label="左";
							key="z";
						}
						:radio_button
						{
							label="右";
							key="y";
						}
					}
				}
			}
		}
	}
	spacer=1;
	ok_cancel;
}