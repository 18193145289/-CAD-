bt:dialog
{
	label="标题";
	:row
	{
		:image
		{
			key="j";
			width=25;
			height=5;
			fixed_height=true;
			color=-2;
		}
		:boxed_column
		{
			label="设置";
			:edit_box
			{
				label="输入文字：";
				key="c";
				value="文字";
				edit_width=10;
			}
			:popup_list
			{
				label="比例大小：";
				list="1:10\n1:20\n1:30\n1:35\n1:50\n1:80\n1:100\n1:150\n1:200";
				key="b";
			}
			:row
			{
				:button
				{
					label="显示比例";
					key="xs";
					width=25;
					fixed_width=true;
				}
				:button
				{
					label="不显示比例";
					key="bxs";
					width=25;
					fixed_width=true;
				}
			}
		}
	}
	spacer=1;
	ok_cancel;
}